{
  "private": false,
  "name": "react-redux-starter-kit",
  "version": "0.0.1",
  "description": "React Redux Starter Kit",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "config": {
    "s3_url": "s3://martin-jujou"
  },
  "scripts": {
    "start": "WATCH=true webpack-dev-server --progress --watch --inline --hot --open --content-base build",
    "build": "webpack --progress",
    "build:prod": "NODE_ENV=production webpack --progress",
    "eslint": "eslint ./src ./tests/unit_tests",
    "deploy:s3": "aws s3 sync ./build $npm_package_config_s3_url --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "stylelint": "stylelint ./src/components/**/*.css ./src/css/*.css",
    "lint": "npm run stylelint && npm run eslint",
    "test:unit": "mocha ./tests/unit_tests/* --compilers js:babel-register,css:./tests/unit_tests/css-null-compiler --require ./tests/unit_tests/setup.js",
    "test": "npm run lint && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jooj123/react-redux-starter-kit.git"
  },
  "keywords": [
    "react",
    "redux",
    "starter",
    "kit",
    "eslint",
    "webpack",
    "babel",
    "babel6",
    "stylelint",
    "postcss"
  ],
  "author": "Martin Jujou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jooj123/react-redux-starter-kit/issues"
  },
  "homepage": "https://github.com/jooj123/react-redux-starter-kit#readme",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "cheerio": "^0.20.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^0.8.5",
    "jsdom": "^8.4.0",
    "mocha": "^2.4.5",
    "postcss-cssnext": "^2.5.1",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.2",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "0.14.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.3",
    "stylelint": "^5.2.1",
    "stylelint-config-standard": "^4.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.14.4",
    "normalize.css": "^4.0.0",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-helmet": "^3.0.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "style-loader": "^0.13.1"
  }
}
